{
	"name": "rails-navigation",
	"displayName": "rails-navigation",
	"description": "Go directly to related views, controllers and models using shortcuts",
	"version": "0.0.1",
	"publisher": "danielsvane",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "rails-navigation.listFiles",
				"title": "List related files"
			}
		],
		"keybindings": [
			{
				"command": "rails-navigation.listFiles",
				"key": "ctrl+alt+l",
				"mac": "cmd+alt+l"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.47.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^10.2.0",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"@types/pluralize": "0.0.29",
		"pluralize": "^8.0.0"
	}
}
